/* pages/feedback/index.less */
page {
  background: #eee;
}
.feedback_wrap {
  padding: 20rpx;
  color: #666;
}
.feedback_wrap .fd_tips {
  display: flex;
  flex-wrap: wrap;
}
.feedback_wrap .fd_tips text {
  width: 30%;
  padding: 10rpx;
  margin: 20rpx 10rpx;
  background: #fff;
  text-align: center;
}
.feedback_wrap .fd_content {
  background: #ffffff;
  margin-top: 20rpx;
}
.feedback_wrap .fd_content .fd_tool {
  display: flex;
  flex-wrap: wrap;
  padding-bottom: 30rpx;
}
.feedback_wrap .fd_content .fd_tool button {
  margin: 0;
  width: 90rpx;
  height: 90rpx;
  padding: 10rpx;
  font-size: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 20rpx;
  margin-top: 20rpx;
  color: #cccccc;
}
.feedback_wrap .fd_content .fd_tool .upImg_item {
  margin-left: 20rpx;
  margin-top: 20rpx;
}
.feedback_wrap .fd_from_btn_wrap {
  margin-top: 20rpx;
  display: flex;
  justify-content: flex-end;
}
.feedback_wrap .fd_from_btn_wrap button {
  margin: 0;
  width: 30%;
}
