/* pages/category/index.less */
page {
  height: 100%;
}
.cates {
  height: 100%;
}
.cates .cates-container {
  height: cal(100vh-90rpx);
  display: flex;
}
.cates .cates-container .left_menu {
  flex: 2;
}
.cates .cates-container .left_menu .menu_item {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
.cates .cates-container .left_menu .active {
  color: var(--themeColor);
  border-left: 5rpx solid currentColor;
}
.cates .cates-container .right_container {
  flex: 5;
}
.cates .cates-container .right_container .goods-group .goods_title {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cates .cates-container .right_container .goods-group .goods_title .delimiter {
  color: #ccc;
  padding: 0 10rpx;
}
.cates .cates-container .right_container .goods-group .goods_list {
  display: flex;
  flex-wrap: wrap;
}
.cates .cates-container .right_container .goods-group .goods_list navigator {
  width: 33.33%;
  text-align: center;
}
.cates .cates-container .right_container .goods-group .goods_list navigator image {
  width: 50%;
}
/*# sourceMappingURL=index.less.map */